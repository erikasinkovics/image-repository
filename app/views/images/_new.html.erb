<%= simple_form_for @image, url: user_images_path(current_user), method: :post do |f| %>
  <div class="image-description">
    <%= f.input :description, as: :text %>
  </div>
  <div class="image-upload">
    <%= f.input :photo, as: :file, public_id: "image-repository-project"%>
    <%= f.submit "Upload Image" %>
  </div>
<% end %>
